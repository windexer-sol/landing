---
import "../styles/global.css";
import { AstroSeo } from '@astrolib/seo';
import Favicons from "./global/Favicons.astro";
---
<AstroSeo
  title="windexer"
  description="Blockchain Data at the Speed of Thought"
  canonical="https://windexeros.xyz"
  openGraph={{
    url: "https://windexeros.xyz",
    title: "windexer",
    description: "Blockchain Data at the Speed of Thought",
    images: [
      {
        url: "icon.svg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "icon.svg" },
    ],
    site_name: "windexer",
  }}
  twitter={{
    handle: "@vivekpal0x",
    site: "https://windexeros.xyz",
    cardType: "summary_large_image",
  }}
/>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width" />
<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta name="author" content="windexer: Data at speed of Thought" />
<meta name="your keywords" content="solana, data, blockchain, windexer, decentralized, data liberation, ai" />


<Favicons/>
<link rel="preconnect" href="https://vivekpal.xyz/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />



